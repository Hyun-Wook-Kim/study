@mixin clearfix {
	&::after {
		content: '';
		display: block;
		clear: both;
	}
}



*{
    box-sizing: border-box;
}

body {
    background-color: #333;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    // height: 100vh;
    // overflow: hidden;
    margin: 0;
    padding: 20px;
}

h2,h3 {
    color: #FFF;
}
h3 {
    margin: 20px 0 5px;
}

section{
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    gap: 10px;
    padding: 5px 0;
    &>div {
        padding: 20px;
        border-radius: 5px;
        background-color: #ffffffee;
        display: flex;
        align-items: center;
        justify-content: center;

    }
}

